	.controller('PraticheProtocollareController', ['$scope', 'Me', 'PraticheProtocollare', '$location', "ModalService", function($scope, Me, PraticheProtocollare, $location, ModalService) {
		$scope.orderByField = 'dataIN';
		$scope.reverseSort = false;
		$scope.me = Me.get();
			
		$scope.pratiche = PraticheProtocollare.query(function() {
			$scope.pratiche.forEach(function(x) {			
				x.rowClass = getTableRowClass(x.idStato);
			});
		});
		$scope.show = function(id) {
			
	/*		ModalService.showModal({
				templateUrl: "public/yesno.html",
				controller: "YesNoController"
			}).then(function(modal) {
				modal.element.modal();
				modal.close.then(function(result) {

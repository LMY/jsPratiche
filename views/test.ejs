				var arrivo = moment(x.dateIN);
				var diff = moment.duration(today.diff(arrivo)).asDays();
				var integDiff = (x.diff?x.diff/2:0);				// /2 = fix alla query che conta doppie le integrazioni!

				if (x.stringStato == "Attesa Integrazione") {
					x.scadenza = "";

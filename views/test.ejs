<html ng-app="app">
<head>
	<title>jsPratiche!</title>
	<style>
	body {
		font: 20px Montserrat, sans-serif;
		line-height: 1.8;
		color: #f5f6f7;
	}
	.bg-4 {
		margin: 30px;
		background-color: #f8f8f8;
		color: #777777;
	}
	.div-border-visible {
		border: 1px solid #aaaaaa;
		padding: 12px;
	}
	.div-border-invisible {
		padding: 12px;
	}
	.div-buttons {
		border-radius: 25px;
		border: 2px solid #73AD21;
		padding: 10px;
		margin: 10px;
	}
	.time-green {
		color: green;
	}
	.time-orange {
		color: orange;
	}
	.time-red {
		color: red;
	}
	.time-danger {
		color: red;
		font-weight: bold;
	}

	.controller('StrumentiStoricoCtrl', ['$scope', 'Me', 'RegistroStrumenti', '$location', function($scope, Me, RegistroStrumenti, $location) {
		$scope.orderByField = 'id';
		$scope.reverseSort = false;
		$scope.me = Me.get();
		$scope.registro = RegistroStrumenti.query();

